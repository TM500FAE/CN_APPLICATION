        <xs:element ref="Media_Stream_Amount_Of_Data" minOccurs="0" maxOccurs="1"/>           <!-- The amount of Kbytes streamed, used if Media_Stream_Duration = Data. Range 1 - 4194304,  (default - 10240 [10Mbytes]). -->
        <xs:element ref="Delay_Between_Sessions" minOccurs="0" maxOccurs="1"/>  <!-- in ms -->
