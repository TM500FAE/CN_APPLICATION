<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="1.00">
 <!-- Common Rules -->
  <xs:simpleType name="Allowable_UE_Range">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="24000"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Allowable_UE_Id_Range">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="23999"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="Allowable_VLAN_Range">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="-1"/>
      <xs:maxInclusive value="4095"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ipv4_Address">
    <xs:restriction base="xs:string">
      <xs:pattern value="((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])\.){3}(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ipv6_Address">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="MAC_Address">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="Mask_Bits">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="65536"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:simpleType name="UE_Groupings">
    <xs:restriction base="xs:string">
      <xs:enumeration value="All"/>
      <xs:enumeration value="None"/>
      <xs:enumeration value="Odd"/>
      <xs:enumeration value="Even"/>
      <xs:enumeration value="List"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="UE_Groupings_no_list">
    <xs:restriction base="xs:string">
      <xs:enumeration value="All"/>
      <xs:enumeration value="None"/>
      <xs:enumeration value="Odd"/>
      <xs:enumeration value="Even"/>
    </xs:restriction>
  </xs:simpleType>
  <!-- Common Elements -->
  <xs:element name="Server_Host_Name">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="255"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Username" default="">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0"/>
        <xs:maxLength value="255"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Password" default="">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0"/>
        <xs:maxLength value="255"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Path">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="0"/>
        <xs:maxLength value="255"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Delay_Between_Commands" default="50">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="3600000"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Delay_Between_Sessions" default="50">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="3600000"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Delay_Between_Requests" default="50">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="600000"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Delay_Between_Connections" default="50">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="600000"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="diversifEye_User">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="6"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="diversifEye_Card">
    <xs:simpleType>
      <xs:restriction base="xs:unsignedInt">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="99"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="diversifEye_Port" default="0">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="0"/>
        <xs:enumeration value="1"/>
        <xs:enumeration value="3"/>
        <xs:enumeration value="4"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="diversifEye_Configuration">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Description" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Use_Reduce" minOccurs="0" maxOccurs="1"/>               <!-- true or false, makes smaller xml output -->
        <xs:element ref="Use_Scaled_Entities" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Use_Scaled_Pad_Increment" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="DiversifEye_Type" minOccurs="1" maxOccurs="1"/>         <!-- 500 or 8400, 500 default -->
        <xs:element ref="Create_Statistic_Groups" minOccurs="0" maxOccurs="1"/>  <!-- true or false, optionally creates the Statistic Groups per client application -->
        <xs:element ref="Normal_Statistics" minOccurs="0" maxOccurs="1"/>        <!-- true or false, optionally uses normal statistics (on by default) -->
        <xs:element ref="Fine_Statistics" minOccurs="0" maxOccurs="1"/>          <!-- true or false, optionally uses fine statistics -->
        <xs:element ref="Latency_Statistics" minOccurs="0" maxOccurs="1"/>       <!-- true or false, optionally uses Latency statistics -->
        <xs:element ref="Background_Ping" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Use_per_Port_VoIP_Srv_Proxy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Pooled_Mode" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Processing_Units" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Global_Settings" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="TM500" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="PPPoE" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="Application_Configuration" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="Threshold_Configuration" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Network_Configuration" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Use_Reduce" type="xs:boolean" default="true"/>
  <xs:element name="Create_Statistic_Groups" type="xs:boolean" default="true"/>
  <xs:element name="Normal_Statistics" type="xs:boolean" default="true"/>
  <xs:element name="Fine_Statistics" type="xs:boolean" default="true"/>
  <xs:element name="Latency_Statistics" type="xs:boolean" default="false"/>
  <xs:element name="Background_Ping">
        <xs:complexType mixed="true">
          <xs:attribute name="Ping_IP_Address" type="xs:string" default="4.2.2.2"/>
          <xs:attribute name="Delay_Between_Pings" default="5000"/>
          <xs:attribute name="Payload_Size" default="100"/>
      </xs:complexType>
  </xs:element>
  <xs:element name="Use_per_Port_VoIP_Srv_Proxy" type="xs:boolean" default="true"/>
  <xs:element name="DiversifEye_Type" default="500">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="500"/>
        <xs:enumeration value="8400"/>
        <xs:enumeration value="1000"/>
        <xs:enumeration value="TeraVM"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Pooled_Mode" type="xs:boolean" default="false"/>
  <xs:element name="Processing_Units" type="xs:unsignedInt" default="0"/>
  <xs:element name="Global_Settings">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Wireless_call_answering_with_183_180_responses" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Send_early_media_if_183_session_progress_received" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Route_SIP_responses_using_VIA_field" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="SIP_Enable_PANI_headers" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="SIP_Access_Type" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="SIP_Access_Info" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="SIP_Access_Value" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="SIP_Number_of_Response_Fields" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Wireless_call_answering_with_183_180_responses" type="xs:boolean" default="false"/>
  <xs:element name="Send_early_media_if_183_session_progress_received" type="xs:boolean" default="false"/>
  <xs:element name="Route_SIP_responses_using_VIA_field" type="xs:boolean" default="false"/>
  <xs:element name="SIP_Enable_PANI_headers" type="xs:boolean" default="false"/>
  <xs:element name="SIP_Access_Type" default="None">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="None"/>
        <xs:enumeration value="IEEE-802.11a"/>
        <xs:enumeration value="IEEE-802.11b"/>
        <xs:enumeration value="3GPP-GERAN"/>
        <xs:enumeration value="3GPP-UTRAN-FDD"/>
        <xs:enumeration value="3GPP-UTRAN-TDD"/>
        <xs:enumeration value="3GPP-CDMA2000"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="SIP_Access_Info" default="None">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="None"/>
        <xs:enumeration value="cgi-3gpp"/>
        <xs:enumeration value="utran-cell-id-3gpp"/>
        <xs:enumeration value="extension-access-info"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="SIP_Access_Value" type="xs:string"/>
  <xs:element name="SIP_Number_of_Response_Fields" type="xs:string"/>
  <xs:element name="TM500">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Total_UEs" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="Minimum_UE_ID_Digits" minOccurs="0" maxOccurs="1"/>    <!-- Between 3 and 5, i.e. 000 and 00000 -->
        <xs:element ref="PDNs_per_UE" minOccurs="1" maxOccurs="1"/>             <!-- Can be 1-8 -->
        <xs:element ref="LAN_IP" minOccurs="1" maxOccurs="1"/>                  <!-- DTE external ip address, configured in TM500 boot-up (card configuration) -->
        <xs:element ref="Test_Module_Port_Mapping" minOccurs="0" maxOccurs="unbounded"/>  <!-- only used for D1000 -->
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Total_UEs" type="Allowable_UE_Range"/>
  <xs:element name="Minimum_UE_ID_Digits" default="4">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="5"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Test_Module_Port_Mapping">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Test_Module_Offset" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Test_Module_Number" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Test_Interface" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Pooled_Unit" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="UE" type="xs:string"/>                                <!-- UE range that the section applies to.  Can be specified as a comma separated list of ranges or UE.  Ranges are defined as a first and last UE separated by '..' (two dots) or '-' (minus sign).  i.e. 0..10,13,18,35..50     The ranges and UEs above Total_UEs are ignored. -->
      <xs:attribute name="PDN" type="xs:string"/>                               <!-- PDN range that the section applies to.  Can be specified as a comma separated list of ranges or PDN.  Ranges are defined as a first and last PDN separated by '..' (two dots) or '-' (minus sign).  i.e. 0..3,5     The ranges and PDNs above PDNs_per_UE  are ignored. -->
    </xs:complexType>
  </xs:element>
  <xs:element name="Test_Module_Offset" default="0">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="0"/>
        <xs:enumeration value="10"/>
        <xs:enumeration value="20"/>
        <xs:enumeration value="30"/>
        <xs:enumeration value="40"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Test_Module_Number" default="10">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="10"/>
        <xs:enumeration value="11"/>
        <xs:enumeration value="12"/>
        <xs:enumeration value="13"/>
        <xs:enumeration value="14"/>
        <xs:enumeration value="15"/>
        <xs:enumeration value="20"/>
        <xs:enumeration value="21"/>
        <xs:enumeration value="22"/>
        <xs:enumeration value="23"/>
        <xs:enumeration value="24"/>
        <xs:enumeration value="25"/>
        <xs:enumeration value="30"/>
        <xs:enumeration value="31"/>
        <xs:enumeration value="32"/>
        <xs:enumeration value="33"/>
        <xs:enumeration value="34"/>
        <xs:enumeration value="35"/>
        <xs:enumeration value="40"/>
        <xs:enumeration value="41"/>
        <xs:enumeration value="42"/>
        <xs:enumeration value="43"/>
        <xs:enumeration value="44"/>
        <xs:enumeration value="45"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Test_Interface" default="0">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:enumeration value="0"/>
        <xs:enumeration value="1"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Pooled_Unit" default="1">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="99"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="PDNs_per_UE">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="8"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="LAN_IP" type="ipv4_Address" default="99.99.99.99"/>
  <xs:element name="Threshold_Configuration">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Threshold" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Threshold">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Name" minOccurs="1" maxOccurs="1"/>                    <!-- A Unique name for the threshold. -->
        <xs:element ref="Rule" minOccurs="1" maxOccurs="1"/>                    <!-- The threshold rule, To be XML compliant use &gt; and &lt; for the greater than and less than -->
        <xs:element ref="Regular_Expression" minOccurs="1" maxOccurs="1"/>      <!-- The regular expression for selecting the targets -->
        <xs:element ref="Start_Test_Delay" minOccurs="1" maxOccurs="1"/>        <!-- in seconds -->
        <xs:element ref="Violation_Trigger" minOccurs="0" maxOccurs="1"/>         <!-- The number of continuous statistical samples for which the Threshold must be violated before a Threshold Violation Event is generated.  Integer between 1 and 10000 -->
        <xs:element ref="Clear_Trigger" minOccurs="0" maxOccurs="1"/>             <!-- The number of continuous statistical samples for which the Threshold must be cleared before a Threshold Clear Event is generated.  Integer between 1 and 10000 -->
        <xs:element ref="Event_Tag" minOccurs="1" maxOccurs="1"/>               <!-- To be XML compliant use &gt; and &lt; for the greater than and less than -->
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Use_Scaled_Entities" type="xs:boolean" default="false"/>
  <xs:element name="Use_Scaled_Pad_Increment" type="xs:boolean" default="false"/>
  <xs:element name="Name" type="xs:string"/>
  <xs:element name="Rule" type="xs:string"/>
  <xs:element name="Regular_Expression" type="xs:string"/>
  <xs:element name="Start_Test_Delay">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="0"/>
        <xs:maxInclusive value="3600"/> <!-- 60 minutes -->
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Violation_Trigger">
    <xs:simpleType>
      <xs:restriction base="xs:integer">
        <xs:minInclusive value="1"/>
        <xs:maxInclusive value="10000"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Clear_Trigger">
      <xs:simpleType>
        <xs:restriction base="xs:integer">
          <xs:minInclusive value="1"/>
          <xs:maxInclusive value="10000"/>
        </xs:restriction>
      </xs:simpleType>
  </xs:element>
  <xs:element name="Event_Tag" type="xs:string"/>
  <xs:element name="Network_Configuration">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="IPv4" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="IPv6" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="TCP_Characteristics" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Flow_Processors" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Domain">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="255"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="SIP_Auth_Username">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:minLength value="1"/>
        <xs:maxLength value="255"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="Flow_Processors">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Ma3" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma4" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma5" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma6" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma7" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma8" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma10" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma11" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma12" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma13" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma14" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma15" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma20" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma21" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma22" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma23" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma24" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma25" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma30" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma31" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma32" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma33" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma34" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma35" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma36" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma37" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma38" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma39" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma40" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma41" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma42" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma43" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma44" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma45" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma46" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma47" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma48" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma49" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma50" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma51" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma52" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma53" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma54" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma55" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma56" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma57" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma58" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="Ma59" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Ma3" type="MAC_Address"/>
  <xs:element name="Ma4" type="MAC_Address"/>
  <xs:element name="Ma5" type="MAC_Address"/>
  <xs:element name="Ma6" type="MAC_Address"/>
  <xs:element name="Ma7" type="MAC_Address"/>
  <xs:element name="Ma8" type="MAC_Address"/>
  <xs:element name="Ma10" type="MAC_Address"/>
  <xs:element name="Ma11" type="MAC_Address"/>
  <xs:element name="Ma12" type="MAC_Address"/>
  <xs:element name="Ma13" type="MAC_Address"/>
  <xs:element name="Ma14" type="MAC_Address"/>
  <xs:element name="Ma15" type="MAC_Address"/>
  <xs:element name="Ma20" type="MAC_Address"/>
  <xs:element name="Ma21" type="MAC_Address"/>
  <xs:element name="Ma22" type="MAC_Address"/>
  <xs:element name="Ma23" type="MAC_Address"/>
  <xs:element name="Ma24" type="MAC_Address"/>
  <xs:element name="Ma25" type="MAC_Address"/>
  <xs:element name="Ma30" type="MAC_Address"/>
  <xs:element name="Ma31" type="MAC_Address"/>
  <xs:element name="Ma32" type="MAC_Address"/>
