        <xs:element ref="Codec_Used_For" minOccurs="0" maxOccurs="1"/>          <!-- Voice, Streaming or PCAP -->
        <xs:element ref="Codec_Encoding_Name" minOccurs="0" maxOccurs="1"/>     <!-- Custom (between 3 and 32 characters) or predefined one ('mpeg4-generic', 'H.264', 'PCMA', 'PCMU', 'G722', 'G723', 'G728', 'G729', 'GSM', 'iLBC') -->
        <xs:element ref="Codec_Media_Type" minOccurs="0" maxOccurs="1"/>        <!-- Only if Codec_Used_For is streaming, 'audio' or 'video' -->
        <xs:element ref="Codec_Payload_Type" minOccurs="0" maxOccurs="1"/>      <!-- Identifies the RTP AVP, range 0-127 (0 - 96 is defined by IANA) -->
        <xs:element ref="Codec_Payload_Size" minOccurs="0" maxOccurs="1"/>      <!-- Payload octets to be carried in each RTP packet, range between 1 and 1446 -->
        <xs:element ref="Codec_ms_Packet" minOccurs="0" maxOccurs="1"/>         <!-- Only if Media_Type is audio, range of 1.0 - 1000000.0 ms -->
        <xs:element ref="Codec_Stream_Rate" minOccurs="0" maxOccurs="1"/>       <!-- Only if Media_Type is Multimedia, in kbits/sec, between 16 and 5000000 -->
        <xs:element ref="Codec_Frequency" minOccurs="0" maxOccurs="1"/>         <!-- The frequency or clock rate for this codec.8000 (default) - 200,000 Hz.-->
        <xs:element ref="Codec_Channels" minOccurs="0" maxOccurs="1"/>          <!-- Only if Media_Type is audio, Number of audio channels for this codec. Default is None, valid values are 1 and 2 -->
        <xs:element ref="Codec_Data_File" minOccurs="0" maxOccurs="1"/>         <!-- Empty for arbitrary data or just the file name or path relative to /home/cli WARNING - If the file does not exist in /home/cli then provisioning WILL fail, MUST contain enough data for least one RTP packet, the file size must be at least the number of bytes specified by the Codec_Payload_Size -->
