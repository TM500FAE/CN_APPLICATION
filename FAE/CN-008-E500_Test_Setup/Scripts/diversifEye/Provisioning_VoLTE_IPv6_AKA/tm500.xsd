        <xs:element ref="Http_Operation" minOccurs="0" maxOccurs="1"/>             <!-- Choice of GET, POST or HEAD -->
        <xs:element ref="Use_TLS" minOccurs="0" maxOccurs="1"/>                    <!-- Enable TLS support, (also changes port number to 443), true of false (default) -->
        <xs:element ref="Path" minOccurs="0" maxOccurs="1"/>                       <!-- Full HTTP get/post/head URI (either relative, or could contain "HTTP://" etc.) -->
