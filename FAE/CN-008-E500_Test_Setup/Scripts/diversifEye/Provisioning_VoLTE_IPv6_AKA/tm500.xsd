         <xs:element ref="Max_Transmit_Buffer_Size" minOccurs="0" maxOccurs="1"/>             <!-- Max Transmit Buffer Size = 524288 (=16*32kiB=512kiB), 65536 to 1073741824 -->
         <xs:element ref="Support_Timestamp_when_requested" minOccurs="0" maxOccurs="1"/>     <!-- Indicates if the receiving end of the TCP will support Timestamping when requested = false (true or false) -->
         <xs:element ref="Request_Timestamp" minOccurs="0" maxOccurs="1"/>                    <!-- Indicates if the TCP timestamp option will be included in the SYN segment of TCP header = false (true or false) -->

      </xs:sequence>
    </xs:complexType>
